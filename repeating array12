#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;

    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int i,j;
    for(i=0;i<n;i++){
        for(j=n;j>0;j--){
            if(a[j]==a[i]){
                goto indexNo;
       }
    }
    }
    indexNo:
    cout<<j<<endl;
    
    return 0;
}
