#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;

    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int S;
    cin>>S;
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(a[j]+a[i]==S){
                cout<<j<<" "<<i<<endl;
            }
       }
       
    }
    
    
    return 0;
}
